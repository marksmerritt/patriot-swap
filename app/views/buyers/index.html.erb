<h1>Buyers#index</h1>

<%= form_tag buyers_path, method: :get do %>
  <%= text_field_tag :q, nil, id: "listing-search-field" %>
  <%= submit_tag "Search", id: "listing-search-submit" %>
<% end %>

<% @listings.each do |listing| %>
  <div>
    <%= link_to listing.title, listing_path(listing), id: "listing-#{listing.id}-show" %>
  </div>
<% end %>
